{
    "resourceType": "StructureDefinition",
    "url": "https://simplifier.net/PSCC-Test/StructureDefinition/Primarydiagnosis ",
    "version": "0.9.0",
    "name": "Primarydiagnosis",
    "status": "active",
    "fhirVersion": "4.0.0",
    "abstract": false,
    "kind": "resource",
    "type": "Condition",
    "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Condition",
    "derivation": "constraint",
    "differential": {
        "element":  [
            {
                "id": "Condition.code",
                "path": "Condition.code",
                "min": 1,
                "max": "1",
                "mustSupport": true,
                "binding": {
                    "strength": "required",
                    "valueSet": "http://terminology.hl7.org/ValueSet/icd-10"
                },
                "comment": "Primary cancer diagnosis code (ICD-10)."
            },
            {
                "id": "Condition.bodySite",
                "path": "Condition.bodySite",
                "min": 1,
                "max": "1",
                "mustSupport": true,
                "comment": "Topographic code (ICD-10) for the primary tumor location."
            },
            {
                "id": "Condition.bodySite.coding:ICD-10-T",
                "path": "Condition.bodySite.coding",
                "sliceName": "ICD-10-T",
                "short": "ICD-10 Topographie",
                "max": "1",
                "mustSupport": true,
                "binding": {
                    "strength": "required",
                    "valueSet": "http://terminology.hl7.org/ValueSet/icd-10-topography"
                },
                "comment": "Include codes from urn:oid:2.16.840.1.113883.6.43.1 where code Regex C\\d\\d\\.\\d(\\d)?"
            },
            {
                "id": "Condition.subject",
                "path": "Condition.subject",
                "min": 1,
                "type":  [
                    {
                        "code": "Reference",
                        "targetProfile":  [
                            "https://simplifier.net/pscc-test/StructureDefinition/patient"
                        ]
                    }
                ],
                "mustSupport": true
            },
            {
                "id": "Condition.onsetDateTime",
                "path": "Condition.onsetDateTime",
                "min": 1,
                "max": "1",
                "short": "Date of cancer diagnosis",
                "mustSupport": true,
                "comment": "Combines cancer onset and diagnosis date."
            },
            {
                "id": "Condition.stage.assessment",
                "path": "Condition.stage.assessment",
                "mustSupport": true,
                "type":  [
                    {
                        "code": "Reference",
                        "targetProfile":  [
                            "https://simplifier.net/pscc-test/tnmstage"
                        ]
                    }
                ]
            },
            {
                "id": "Condition.evidence",
                "path": "Condition.evidence",
                "mustSupport": true
            },
            {
                "id": "Condition.evidence.detail",
                "path": "Condition.evidence.detail",
                "type":  [
                    {
                        "code": "Reference",
                        "targetProfile":  [
                            "https://simplifier.net/PSCC-Test/ValueSet-ICD-O-3-Morphology",
                            "https://simplifier.net/PSCC-Test/StructureDefinition/Histologie"
                        ]
                    }
                ],
                "mustSupport": false
            },
            {
                "id": "Condition.evidence.detail.reference",
                "path": "Condition.evidence.detail.reference",
                "min": 1
            }
        ]
    }
}