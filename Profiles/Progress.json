    "differential": {
        "element":  [
            {
                "id": "ClinicalImpression.status",
                "path": "ClinicalImpression.status",
                "mustSupport": true
            },
            {
                "id": "ClinicalImpression.subject",
                "path": "ClinicalImpression.subject",
                "type":  [
                    {
                        "code": "Reference",
                        "targetProfile":  [
                            "https://simplifier.net/PSCC-Test/StructureDefinition/Patient"
                        ]
                    }
                ],
                "mustSupport": true
            },
            {
                "id": "ClinicalImpression.encounter",
                "path": "ClinicalImpression.encounter",
                "type":  [
                    {
                        "code": "Reference",
                        "targetProfile":  [
                            "https://simplifier.net/PSCC-Test/StructureDefinition/Encounter"
                        ]
                    }
                ],
                "mustSupport": true
            },
            {
                "id": "ClinicalImpression.effectiveDateTime",
                "path": "ClinicalImpression.effectiveDateTime",
                "min": 1,
                "max": "1",
                "short": "Diagnosis Date",
                "comment": "Date of diagnosis (YYYY/MM/DD)."
            },
            {
                "id": "ClinicalImpression.previous",
                "path": "ClinicalImpression.previous",
                "type":  [
                    {
                        "code": "Reference",
                        "targetProfile":  [
                            "https://simplifier.net/PSCC-Test/StructureDefinition/Progress"
                        ]
                    }
                ]
            },
            {
                "id": "ClinicalImpression.problem",
                "path": "ClinicalImpression.problem",
                "type":  [
                    {
                        "code": "Reference",
                        "targetProfile":  [
                            "https://simplifier.net/PSCC-Test/StructureDefinition/Primaerdiagnosis"
                        ]
                    }
                ],
                "mustSupport": true
            },
            {
                "id": "ClinicalImpression.finding.item",
                "path": "ClinicalImpression.finding.item",
                "min": 1,
                "max": "1",
                "binding": {
                    "strength": "required",
                    "valueSet": "https://simplifier.net/PSCC-Test/ValueSet/TumorEventType"
                },
                "comment": "Type of tumor event (primary, recurrence, metastasis, unknown)."
            }
        ]
    }
